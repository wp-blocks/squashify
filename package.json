{
  "name": "squashify",
  "version": "0.1.2",
  "description": "A Node.js command-line tool and script to compress and optimize images, using different algorithms to reduce file size",
  "license": "GPL-2.0-or-later",
  "contributors": [
    "Erik Golinelli <erik@codekraft.it> (https://codekraft.it/)",
    "John Hooks <bitmachina@outlook.com> (https://johnhooks.io/)"
  ],
  "homepage": "https://github.com/wp-blocks/squashify/#readme",
  "bugs": {
    "url": "https://github.com/wp-blocks/squashify/issues"
  },
  "scripts": {
    "build": "tsc --build",
    "watch": "tsc --watch",
    "clean": "tsc --build --clean",
    "doc": "npx typedoc src/*",
    "squashify": "node ./lib/index.js --interactive",
    "test": "jest --silent=false --coverage --verbose"
  },
  "files": [
    "lib",
    "tests",
    "package.json",
    ".gitignore",
    "LICENSE",
    "readme.md"
  ],
  "keywords": [
    "image",
    "compression",
    "optimization",
    "performance",
    "CLI",
    "tool",
    "avif",
    "webp",
    "svg",
    "development",
    "frontend",
    "SEO"
  ],
  "main": "./lib/index.js",
  "bin": "./lib/index.js",
  "dependencies": {
    "ini": "^4.1.0",
    "prompts": "^2.4.2",
    "sharp": "^0.32.1",
    "svgo": "^3.0.2",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-typescript": "^7.18.6",
    "@jest/globals": "^29.3.1",
    "@types/ini": "^1.3.31",
    "@types/jest": "^29.2.6",
    "@types/node": "^18.11.18",
    "@types/prompts": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.5",
    "eslint": "^8.40.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.3.1",
    "prettier": "^2.8.8",
    "typedoc": "^0.24.7",
    "typescript": "^5.0.4"
  }
}
